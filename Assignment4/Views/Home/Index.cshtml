@{
    ViewBag.Title = "Top 5 Restaurants in Provo";
}
@model IEnumerable<string>

<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
    </head>

<body>
    <header>
        <h4>These are the top 5 restaurants in Provo according to me</h4>
    </header>
    <div>
        <ul>
            @foreach (string i in Model)
            {
                <li class="list-group-item">@i</li>
            }
        </ul>
    </div>
</body>

</html>
